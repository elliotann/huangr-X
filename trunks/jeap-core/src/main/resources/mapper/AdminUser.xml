<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.easysoft.jeap.core.member.dao.IAdminUserDao">
    <resultMap id="adminUserMap" type="AdminUser">
        <id property="id" column="user_id"/>
        <id property="username" column="username"/>
        <id property="password" column="password"/>
    </resultMap>
    <insert id="save" parameterType="AdminUser">
        insert t_adminuser (username,password,status) values (#{username},#{password},#{status})
    </insert>
    <select id="queryForPage" parameterType="AdminUser" resultMap="adminUserMap">
        select * from t_adminuser
    </select>
</mapper>
